  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">= 4.5.0"
    }
  ],
  "template-url": "https://github.com/puppetlabs/pdk-templates#2.0.0",
  "pdk-version": "1.18.1",
  "template-ref": "tags/2.0.0-0-ge838f1d"
