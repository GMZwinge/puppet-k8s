{
  "name": "liuit-k8s",
  "version": "0.0.1",
  "author": "LiU-IT",
  "summary": "Module for installing and managing plain Kubernetes installs",
  "license": "MIT",
  "source": "https://gitlab.it.liu.se/pm-liuit/k8s/tree/production",
  "project_page": "https://gitlab.it.liu.se/pm-liuit/k8s",
  "issues_url": "https://gitlab.it.liu.se/pm-liuit/k8s/issues",
  "dependencies": [
    {
      "name": "puppet-archive",
      "version_requirement": ">= 4.0.0 < 7.0.0"
    },
    {
      "name": "puppetlabs-stdlib",
      "version_requirement": ">= 5.0.0 < 9.0.0"
    },
    {
      "name": "puppet-systemd",
      "version_requirement": ">= 2.0.0 < 4.0.0"
    }
  ],
  "operatingsystem_support": [
    {
