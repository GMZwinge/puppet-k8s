      "operatingsystemrelease": [
        "18.04",
        "20.04",
        "20.10"
      ]
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">= 6.0.0"
    }
  ],
  "template-url": "https://github.com/puppetlabs/pdk-templates#2.0.0",
  "pdk-version": "2.1.0",
  "template-ref": "tags/2.0.0-0-ge838f1d"
